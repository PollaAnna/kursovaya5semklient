<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.kursovayaclient1.login.LoginController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="42.0" layoutY="43.0" text="Username" />
        <TextField layoutX="120.0" layoutY="40.0" prefWidth="150.0" fx:id="loginTf"/>
        <Label layoutX="42.0" layoutY="93.0" text="Password" />
        <PasswordField layoutX="120.0" layoutY="90.0" prefWidth="150.0" fx:id="passwTf" />
        <Button layoutX="120.0" layoutY="140.0" mnemonicParsing="false" text="Login" onAction="#submit" />
        <Label layoutX="42.0" layoutY="43.0" text="" fx:id="errrField"/>
    </children>
